{
  "name": "png-cutter",
  "version": "1.0.0",
  "description": "cut out the transparent pixels around the png image to reduce the size of the png image",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShiverZheng/png-cutter.git"
  },
  "keywords": [
    "png",
    "typescript",
    "cut",
    "transparent pixels"
  ],
  "author": "Shiver Zheng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShiverZheng/png-cutter/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/ShiverZheng/png-cutter#readme",
  "devDependencies": {
    "@types/node": "^14.0.11",
    "prettier": "^1.19.1",
    "should": "^13.2.3",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "webpack-cli": "^3.3.11",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  }
}
